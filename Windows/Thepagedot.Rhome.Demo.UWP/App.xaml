<Application
    x:Class="Thepagedot.Rhome.App.UWP.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Thepagedot.Rhome.App.UWP"
    RequestedTheme="Light">

    <Application.Resources>

        <local:Bootstrapper x:Key="Bootstrapper" />

        <!--  Colors  -->
        <Color x:Key="HomeMaticBlue">#FF071B7C</Color>

        <SolidColorBrush x:Key="ApplicationPageBackgroundThemeBrush" Color="{ThemeResource SystemChromeMediumLowColor}" />

    </Application.Resources>

</Application>
